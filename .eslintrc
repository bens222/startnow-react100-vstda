{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-curly-spacing": [2, "always"],
    "react/prefer-stateless-function": [0],
    "comma-dangle": "off",
    "react/prop-types": 0
  },
  "env": {
    "jasmine": true,
    "browser": true
  }
}
